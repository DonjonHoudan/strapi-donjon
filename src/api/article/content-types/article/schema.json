{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string",
      "required": true
    },
    "contenu": {
      "type": "blocks",
      "required": true
    },
    "programmation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::programmation.programmation"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "lien": {
      "type": "string",
      "regex": "^https?:\\/\\/(?:www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}(?:[0-9]|\\.[a-zA-Z0-9()]{1,6})\\b(?:[-a-zA-Z0-9()@:%_\\+.~#?&\\/=]*)$"
    },
    "slug": {
      "type": "uid",
      "targetField": "titre"
    }
  }
}
